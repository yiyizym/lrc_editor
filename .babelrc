{
    "presets": [
        ["@babel/preset-env", {"useBuiltIns": false}],
        "@babel/preset-react"
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators", {"legacy": true}],
        ["@babel/plugin-proposal-class-properties", {"loose": true }],
        ["@babel/plugin-transform-runtime", { "regenerator": true }]
    ]
}